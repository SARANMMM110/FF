{
  "name": "mocha-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@hono/zod-validator": "^0.5.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.16.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.6.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "hono": "4.7.7",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.510.0",
    "mysql2": "^3.16.0",
    "pg": "^8.17.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-is": "^19.2.3",
    "react-router": "^7.5.3",
    "recharts": "^3.3.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.12.0",
    "@cloudflare/workers-types": "^4.20260114.0",
    "@eslint/js": "9.25.1",
    "@types/node": "22.14.1",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "eslint": "9.25.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "globals": "15.15.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.21.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "^7.1.3",
    "wrangler": "^4.33.0"
  },
  "scripts": {
    "build": "tsc -b && vite build",
    "build:frontend": "vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "cf-typegen": "wrangler types",
    "check": "tsc && vite build && wrangler deploy --dry-run",
    "dev": "vite",
    "dev:frontend": "vite",
    "dev:backend": "wrangler dev",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "dev:full": "concurrently \"npm run dev:node\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\"",
    "dev:node": "tsx watch src/server/index.ts",
    "start": "node dist/server/index.js",
    "start:prod": "NODE_ENV=production node dist/server/index.js",
    "start:tsx": "NODE_ENV=production tsx src/server/index.ts",
    "migrate": "tsx scripts/migrate.ts",
    "lint": "eslint ."
  }
}
